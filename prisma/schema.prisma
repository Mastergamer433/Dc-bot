// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Economy{
  id String @id
  username String
  balance INT
}

model Cooldown{
  id String @id
  expiry INT
  command String
}
